<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/placeHolder_LabelAL"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:text="@string/placeHolder_label"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.247" />


    <EditText
        android:id="@+id/etLabelAl"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:layout_marginStart="52dp"
        android:ems="10"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/placeHolder_LabelAL"
        app:layout_constraintStart_toEndOf="@+id/placeHolder_LabelAL"
        app:layout_constraintTop_toTopOf="@+id/placeHolder_LabelAL"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/placeHolder_DivisionAL"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginTop="52dp"
        android:text="@string/placeHolder_divison"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/placeHolder_LabelAL"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/placeHolder_LabelAL"
        app:layout_constraintTop_toBottomOf="@+id/placeHolder_LabelAL" />


    <EditText
        android:id="@+id/etDivisionAL"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:ems="10"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/placeHolder_DivisionAL"
        app:layout_constraintEnd_toEndOf="@+id/etLabelAl"
        app:layout_constraintStart_toStartOf="@+id/etLabelAl"
        app:layout_constraintTop_toTopOf="@+id/placeHolder_DivisionAL"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/placeHolder_PLaceAl"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginTop="52dp"
        android:text="@string/placeHolder_place"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/placeHolder_DivisionAL"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/placeHolder_DivisionAL"
        app:layout_constraintTop_toBottomOf="@+id/placeHolder_DivisionAL" />

    <EditText
        android:id="@+id/etPlaceAL"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintBottom_toBottomOf="@+id/placeHolder_PLaceAl"
        app:layout_constraintEnd_toEndOf="@+id/etDivisionAL"
        app:layout_constraintStart_toStartOf="@+id/etDivisionAL"
        app:layout_constraintTop_toTopOf="@+id/placeHolder_PLaceAl"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/btSaveLocationAL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="save"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/placeHolder_PLaceAl"
        app:layout_constraintVertical_bias="0.412" />


</androidx.constraintlayout.widget.ConstraintLayout>